{
  "name": "web-touchwiz-clock",
  "version": "0.0.0",
  "description": "Recreation of the default Clock app found on old Samsung devices with the TouchWiz UI using HTML, CSS, and JS.",
  "homepage": "https://github.com/Talon125/web-touchwiz-clock#readme",
  "bugs": {
    "url": "https://github.com/Talon125/web-touchwiz-clock/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Talon125/web-touchwiz-clock.git"
  },
  "license": "AGPL-3.0-or-later",
  "author": "Talon",
  "type": "module",
  "main": "index.html",
  "scripts": {
    "start": "live-server",
    "lint:js": "standard **/*.js",
    "lint:css": "stylelint '**/*.css'",
    "lint:html": "htmlhint '**/*.html'",
    "lint:fix:js": "standard --fix **/*.js",
    "lint:fix:css": "stylelint --fix '**/*.css'"
  },
  "devDependencies": {
    "htmlhint": "^1.8.0",
    "live-server": "^1.2.2",
    "standard": "^17.1.2",
    "stylelint": "^17.0.0",
    "stylelint-config-concentric-order": "^5.2.1",
    "stylelint-config-recommended": "^18.0.0",
    "stylelint-config-standard": "^40.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-high-performance-animation": "^1.11.0",
    "stylelint-media-use-custom-media": "^4.1.0",
    "stylelint-no-browser-hacks": "^2.0.0",
    "stylelint-no-unresolved-module": "^2.5.2",
    "stylelint-no-unsupported-browser-features": "^8.0.5",
    "stylelint-plugin-defensive-css": "^1.0.4",
    "stylelint-plugin-logical-css": "^1.2.3",
    "stylelint-plugin-use-baseline": "^1.1.6",
    "stylelint-use-nesting": "^6.0.1",
    "stylelint-value-no-unknown-custom-properties": "^6.1.0"
  }
}
